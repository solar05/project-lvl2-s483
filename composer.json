{
    "name": "artems/gendiff",
    "description": "This package can find difference between two text object in many formats like yaml, json e.t.c.",
    "type": "project",
    "scripts": {
        "phpcs": "phpcs",
        "phpunit": "phpunit",
        "test": "phpunit tests"
    },
    "bin": [
        "bin/gendiff"
    ],
    "autoload": {
        "files": [
