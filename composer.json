        "docopt/docopt": "^1.0",
        "funct/funct": "^1.5",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4",
        "phpunit/phpunit": "^8.2"
    },
